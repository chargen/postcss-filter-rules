{
  "name": "postcss-filter-rules",
  "version": "0.2.1",
  "description": "PostCSS plugin to filter rules by applying a callback function on each selector. Can be used to filter out individual rules or remove all rules besides those you wish to keep.",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "rules",
    "selectors",
    "filter"
  ],
  "author": "Lachlan McDonald <lachlan@radelaide.net>",
  "license": "MIT",
  "repository": "lachlanmcdonald/postcss-filter-rules",
  "bugs": {
    "url": "https://github.com/lachlanmcdonald/postcss-filter-rules/issues"
  },
  "homepage": "https://github.com/lachlanmcdonald/postcss-filter-rules",
  "dependencies": {
    "postcss": "^5.2.6"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-postcss": "^2.0.2",
    "jest": "^17.0.2",
    "postcss-selector-parser": "^2.2.3"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss/es5",
    "env": {
      "jest": true
    }
  }
}
